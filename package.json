{
  "name": "adobe-ims-servicetoken",
  "version": "2.0.5",
  "description": "Adobe IMS Service Token lifecycle management",
  "source": "src/ims.js",
  "main": "dist/adobe-ims-servicetoken",
  "exports": {
    "import": "./dist/adobe-ims-servicetoken.js",
    "require": "./dist/adobe-ims-servicetoken.cjs"
  },
  "type": "module",
  "sourceType": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "npm run lint && npm run rollup",
    "changelog": "auto-changelog -p",
    "lint": "eslint *.js src/*.js",
    "rollup": "rollup --config",
    "test": "npm run lint",
    "types": "npx -p typescript tsc src/*.js --declaration --allowJs --emitDeclarationOnly --outDir types"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/avoidwork/adobe-ims-servicetoken.git"
  },
  "keywords": [
    "Adobe",
    "IMS",
    "service",
    "token",
    "refresh",
    "lifecycle",
    "management"
  ],
  "author": "Jason Mulligan <jason.mulligan@avoidwork.com>",
  "license": "BSD-3-Clause",
  "dependencies": {
    "form-data": "^4.0.0",
    "murmurhash3js": "^3.0.1",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.0",
    "auto-changelog": "^2.4.0",
    "eslint": "^8.37.0",
    "rollup": "^3.20.2",
    "typescript": "^5.0.3"
  }
}
